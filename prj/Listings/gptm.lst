N                         (x == GPTM_FLAG_CH2CC) || \
N                         (x == GPTM_FLAG_CH3CC) || \
N                         (x == GPTM_FLAG_CH0OC) || \
N                         (x == GPTM_FLAG_CH1OC) || \
N                         (x == GPTM_FLAG_CH2OC) || \
N                         (x == GPTM_FLAG_CH3OC) || \
N                         (x == GPTM_FLAG_UEV) || \
N                         (x == GPTM_FLAG_TEV))
X#define IS_GPTM_FLAG(x) ((x == GPTM_FLAG_CH0CC) ||                          (x == GPTM_FLAG_CH1CC) ||                          (x == GPTM_FLAG_CH2CC) ||                          (x == GPTM_FLAG_CH3CC) ||                          (x == GPTM_FLAG_CH0OC) ||                          (x == GPTM_FLAG_CH1OC) ||                          (x == GPTM_FLAG_CH2OC) ||                          (x == GPTM_FLAG_CH3OC) ||                          (x == GPTM_FLAG_UEV) ||                          (x == GPTM_FLAG_TEV))
