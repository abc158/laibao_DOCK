N                             (x == GPTM_INT_CH3CC) || \
N                             (x == GPTM_INT_UEV) || \
N                             (x == GPTM_INT_TEV))
X#define IS_GPTM_GET_INT(x)  ((x == GPTM_INT_CH0CC) ||                              (x == GPTM_INT_CH1CC) ||                              (x == GPTM_INT_CH2CC) ||                              (x == GPTM_INT_CH3CC) ||                              (x == GPTM_INT_UEV) ||                              (x == GPTM_INT_TEV))
N
N/* check parameter of the GPTM STI selection */
N#define IS_GPTM_TRSEL(x) ((x == GPTM_TRSEL_UEVG) || \
