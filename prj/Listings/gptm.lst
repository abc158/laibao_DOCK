N
N/* check parameter of the event */
N#define IS_GPTM_EVENT(x)  (((x & 0xFFFFFAF0) == 0x0000) && (x != 0x0000))
N
N/* check parameter of the GPTM master mode selection */
N#define IS_GPTM_MMSEL(x)  (((x) == GPTM_MMSEL_RESET) || \
N                           ((x) == GPTM_MMSEL_ENABLE) || \
N                           ((x) == GPTM_MMSEL_UPDATE) || \
N                           ((x) == GPTM_MMSEL_CH0CC) || \
N                           ((x) == GPTM_MMSEL_CH0OREF) || \
N                           ((x) == GPTM_MMSEL_CH1OREF) || \
N                           ((x) == GPTM_MMSEL_CH2OREF) || \
