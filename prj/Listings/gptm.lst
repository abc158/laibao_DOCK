N/* Definitions of GPTM_INT */
N#define GPTM_INT_CH0CC        0x0001  /*!< Channel 0 capture/compare interrupt */
N#define GPTM_INT_CH1CC        0x0002  /*!< Channel 1 capture/compare interrupt */
N#define GPTM_INT_CH2CC        0x0004  /*!< Channel 2 capture/compare interrupt */
N#define GPTM_INT_CH3CC        0x0008  /*!< Channel 3 capture/compare interrupt */
N#define GPTM_INT_UEV          0x0100  /*!< Update interrupt */
N#define GPTM_INT_TEV          0x0400  /*!< Trigger interrupt */
N
N/* GPTM channel output modes */
N#define GPTM_OM_MATCH_NOCHANGE        0x0000  /*!< GPTM channel output no change on match */
N#define GPTM_OM_MATCH_INACTIVE        0x0001  /*!< GPTM channel output inactive level on match */
N#define GPTM_OM_MATCH_ACTIVE          0x0002  /*!< GPTM channel output active level on match */
N#define GPTM_OM_MATCH_TOGGLE          0x0003  /*!< GPTM channel output toggle on match */
N#define GPTM_OM_FORCED_INACTIVE       0x0004  /*!< GPTM channel output forced inactive level */
N#define GPTM_OM_FORCED_ACTIVE         0x0005  /*!< GPTM channel output forced active level */
N#define GPTM_OM_PWM1                  0x0006  /*!< GPTM channel pwm1 output mode */
N#define GPTM_OM_PWM2                  0x0007  /*!< GPTM channel pwm2 output mode */
N#define GPTM_OM_ASYMMETRIC_PWM1       0x0106  /*!< GPTM channel asymmetric pwm1 output mode */
N#define GPTM_OM_ASYMMETRIC_PWM2       0x0107  /*!< GPTM channel asymmetric pwm2 output mode */
N
N/* Definitions of GPTM_PDMA */
N#define GPTM_PDMA_CH0CC        0x00010000  /*!< Channel 0 capture/compare PDMA request */
N#define GPTM_PDMA_CH1CC        0x00020000  /*!< Channel 1 capture/compare PDMA request */
N#define GPTM_PDMA_CH2CC        0x00040000  /*!< Channel 2 capture/compare PDMA request */
N#define GPTM_PDMA_CH3CC        0x00080000  /*!< Channel 3 capture/compare PDMA request */
N#define GPTM_PDMA_UEV          0x01000000  /*!< Update PDMA request */
N#define GPTM_PDMA_TEV          0x04000000  /*!< Trigger PDMA request */
N
N/* Definitions of GPTM_EVENT */
N#define GPTM_EVENT_CH0CC      0x0001  /*!< Channel 0 capture/compare event */
N#define GPTM_EVENT_CH1CC      0x0002  /*!< Channel 1 capture/compare event */
N#define GPTM_EVENT_CH2CC      0x0004  /*!< Channel 2 capture/compare event */
N#define GPTM_EVENT_CH3CC      0x0008  /*!< Channel 3 capture/compare event */
N#define GPTM_EVENT_UEV        0x0100  /*!< Update event */
N#define GPTM_EVENT_TEV        0x0400  /*!< Trigger event */
N
N/* Definitions of GPTM_FLAG */
N#define GPTM_FLAG_CH0CC       0x0001  /*!< Channel 0 capture/compare flag */
N#define GPTM_FLAG_CH1CC       0x0002  /*!< Channel 1 capture/compare flag */
N#define GPTM_FLAG_CH2CC       0x0004  /*!< Channel 2 capture/compare flag */
N#define GPTM_FLAG_CH3CC       0x0008  /*!< Channel 3 capture/compare flag */
N#define GPTM_FLAG_CH0OC       0x0010  /*!< Channel 0 over capture flag */
N#define GPTM_FLAG_CH1OC       0x0020  /*!< Channel 1 over capture flag */
N#define GPTM_FLAG_CH2OC       0x0040  /*!< Channel 2 over capture flag */
N#define GPTM_FLAG_CH3OC       0x0080  /*!< Channel 3 over capture flag */
N#define GPTM_FLAG_UEV         0x0100  /*!< Update flag */
N#define GPTM_FLAG_TEV         0x0400  /*!< Trigger flag */
N
N/* check parameter of the GPTMx */
N#define IS_GPTM(x) ((x == AM_GPTM0) || (x == AM_GPTM1))
N
N/* check parameter of the output compare mode */
N#define IS_GPTM_OM_CMP(x) (((x) == GPTM_OM_MATCH_NOCHANGE) || \
N                           ((x) == GPTM_OM_MATCH_INACTIVE) || \
N                           ((x) == GPTM_OM_MATCH_ACTIVE) || \
N                           ((x) == GPTM_OM_MATCH_TOGGLE) || \
N                           ((x) == GPTM_OM_PWM1) || \
N                           ((x) == GPTM_OM_PWM2))
X#define IS_GPTM_OM_CMP(x) (((x) == GPTM_OM_MATCH_NOCHANGE) ||                            ((x) == GPTM_OM_MATCH_INACTIVE) ||                            ((x) == GPTM_OM_MATCH_ACTIVE) ||                            ((x) == GPTM_OM_MATCH_TOGGLE) ||                            ((x) == GPTM_OM_PWM1) ||                            ((x) == GPTM_OM_PWM2))
N
N/* check parameter of the output mode */
N#define IS_GPTM_OM(x) (((x) == GPTM_OM_MATCH_NOCHANGE) || \
N                       ((x) == GPTM_OM_MATCH_INACTIVE) || \
N                       ((x) == GPTM_OM_MATCH_ACTIVE) || \
N                       ((x) == GPTM_OM_MATCH_TOGGLE) || \
N                       ((x) == GPTM_OM_PWM1) || \
N                       ((x) == GPTM_OM_PWM2) || \
N                       ((x) == GPTM_OM_FORCED_INACTIVE) || \
N                       ((x) == GPTM_OM_FORCED_ACTIVE) || \
N                       ((x) == GPTM_OM_ASYMMETRIC_PWM1) || \
N                       ((x) == GPTM_OM_ASYMMETRIC_PWM2))
X#define IS_GPTM_OM(x) (((x) == GPTM_OM_MATCH_NOCHANGE) ||                        ((x) == GPTM_OM_MATCH_INACTIVE) ||                        ((x) == GPTM_OM_MATCH_ACTIVE) ||                        ((x) == GPTM_OM_MATCH_TOGGLE) ||                        ((x) == GPTM_OM_PWM1) ||                        ((x) == GPTM_OM_PWM2) ||                        ((x) == GPTM_OM_FORCED_INACTIVE) ||                        ((x) == GPTM_OM_FORCED_ACTIVE) ||                        ((x) == GPTM_OM_ASYMMETRIC_PWM1) ||                        ((x) == GPTM_OM_ASYMMETRIC_PWM2))
N
N/* check parameter of the channel */
N#define IS_GPTM_CH(x) (((x) == GPTM_CH_0) || ((x) == GPTM_CH_1) || \
N                       ((x) == GPTM_CH_2) || ((x) == GPTM_CH_3))
X#define IS_GPTM_CH(x) (((x) == GPTM_CH_0) || ((x) == GPTM_CH_1) ||                        ((x) == GPTM_CH_2) || ((x) == GPTM_CH_3))
N
N/* check parameter of the channel for PWM input function */
N#define IS_GPTM_CH_PWMI(x) (((x) == GPTM_CH_0) || ((x) == GPTM_CH_1))
N
N/* check parameter of the clock divider */
N#define IS_GPTM_CKDIV(x)  ((x == GPTM_CKDIV_OFF) || \
N                           (x == GPTM_CKDIV_2) || \
N                           (x == GPTM_CKDIV_4))
X#define IS_GPTM_CKDIV(x)  ((x == GPTM_CKDIV_OFF) ||                            (x == GPTM_CKDIV_2) ||                            (x == GPTM_CKDIV_4))
N
N/* check parameter of the counter mode */
N#define IS_GPTM_CNT_MODE(x) ((x == GPTM_CNT_MODE_UP) ||  \
N                             (x == GPTM_CNT_MODE_CA1) || \
N                             (x == GPTM_CNT_MODE_CA2) || \
N                             (x == GPTM_CNT_MODE_CA3) || \
N                             (x == GPTM_CNT_MODE_DOWN))
X#define IS_GPTM_CNT_MODE(x) ((x == GPTM_CNT_MODE_UP) ||                               (x == GPTM_CNT_MODE_CA1) ||                              (x == GPTM_CNT_MODE_CA2) ||                              (x == GPTM_CNT_MODE_CA3) ||                              (x == GPTM_CNT_MODE_DOWN))
N
N/* check parameter of the channel polarity */
N#define IS_GPTM_CHP(x)    ((x == GPTM_CHP_NONINVERTED) || (x == GPTM_CHP_INVERTED))
N
N/* check parameter of the channel control */
N#define IS_GPTM_CHCTL(x)  ((x == GPTM_CHCTL_DISABLE) || (x == GPTM_CHCTL_ENABLE))
N
N/* check parameter of the channel capture / compare PDMA selection */
N#define IS_GPTM_CHCCDS(x) ((x == GPTM_CHCCDS_CHCCEV) || (x == GPTM_CHCCDS_UEV))
N
N/* check parameter of the channel input selection */
N#define IS_GPTM_CHCCS(x)  ((x == GPTM_CHCCS_DIRECT) || \
N                           (x == GPTM_CHCCS_INDIRECT) || \
N                           (x == GPTM_CHCCS_TRCED))
X#define IS_GPTM_CHCCS(x)  ((x == GPTM_CHCCS_DIRECT) ||                            (x == GPTM_CHCCS_INDIRECT) ||                            (x == GPTM_CHCCS_TRCED))
N
N/* check parameter of the channel capture prescaler */
N#define IS_GPTM_CHPSC(x)  ((x == GPTM_CHPSC_OFF) || \
N                           (x == GPTM_CHPSC_2) || \
N                           (x == GPTM_CHPSC_4) || \
N                           (x == GPTM_CHPSC_8))
X#define IS_GPTM_CHPSC(x)  ((x == GPTM_CHPSC_OFF) ||                            (x == GPTM_CHPSC_2) ||                            (x == GPTM_CHPSC_4) ||                            (x == GPTM_CHPSC_8))
N
N/* check parameter of the ETI prescaler */
N#define IS_GPTM_ETIPSC(x) ((x == GPTM_ETIPSC_OFF) || \
N                           (x == GPTM_ETIPSC_2) || \
N                           (x == GPTM_ETIPSC_4) || \
N                           (x == GPTM_ETIPSC_8))
X#define IS_GPTM_ETIPSC(x) ((x == GPTM_ETIPSC_OFF) ||                            (x == GPTM_ETIPSC_2) ||                            (x == GPTM_ETIPSC_4) ||                            (x == GPTM_ETIPSC_8))
N
N/* check parameter of the GPTM interrupt */
N#define IS_GPTM_INT(x) (((x & 0xFFFFFAF0) == 0x0) && (x != 0))
N
N/* check parameter of the GPTM PDMA request */
N#define IS_GPTM_PDMA(x) (((x & 0xFAF0FFFF) == 0x0) && (x != 0))
N
N/* check parameter of the GPTM interrupt for \ref GPTM_GetIntStatus function */
N#define IS_GPTM_GET_INT(x)  ((x == GPTM_INT_CH0CC) || \
