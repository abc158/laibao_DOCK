N/*********************************************************************************************************//**
N * @brief Initialize the GPTMx channel N output.
N * @param GPTMx: where GPTMx is the selected GPTM from the GPTM peripheral, x can be 0~3.
N * @param OutInit: Point to a \ref GPTM_OutputInitTypeDef structure that contains
N                   the configuration information.
N * @retval None
N ************************************************************************************************************/
Nvoid GPTM_OutputInit(AM_GPTM_TypeDef* GPTMx, GPTM_OutputInitTypeDef* OutInit)
N{
N  vu32 *pOcfr = (vu32*)&GPTMx->CH0OCFR + OutInit->Channel;
N  vu32 *pCcr = (vu32*)&GPTMx->CH0CCR + OutInit->Channel;
N  vu32 *pAcr = (vu32*)&GPTMx->CH0ACR + OutInit->Channel;
N  u8 bChPos = OutInit->Channel << 1;
N  u32 wTmpMask = ~(0x1ul << bChPos);
N
N  /* Disable the Channel */
N  GPTMx->CHCTR &= wTmpMask;
N
N  /* Set the Output Compare Polarity */
N  GPTMx->CHPOLR &= wTmpMask;
N  GPTMx->CHPOLR |= (u32)OutInit->Polarity << bChPos;
N
N  /* Select the Output Compare Mode */
N  *pOcfr &= CHOCFR_CHOM_MASK;
X  *pOcfr &= ~0x00000107ul;
N  *pOcfr |= OutInit->OutputMode;
N
N  /* Set the Capture Compare Register value */
N  *pCcr = OutInit->Compare;
N
N  /* Set the Asymmetric Compare Register value */
N  *pAcr = OutInit->AsymmetricCompare;
N
N  /* Set the channel state */
N  GPTMx->CHCTR |= (u32)OutInit->Control << bChPos;
