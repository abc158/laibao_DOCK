N/*********************************************************************************************************//**
N * @brief Configure encoder interface of the GPTMx.
N * @param GPTMx: where GPTMx is the selected GPTM from the GPTM peripheral, x can be 0~3.
N * @param DecoderMod: Specify the GPTMx decoder mode.
N *        This parameter can be one of the following values:
N *        @arg GPTM_SMSEL_DECODER1 : Counter counts on CH0 edge depending on CH1 level.
N *        @arg GPTM_SMSEL_DECODER2 : Counter counts on CH1 edge depending on CH0 level.
N *        @arg GPTM_SMSEL_DECODER3 : Counter counts on both CH0 and CH1 edges depending on
N *             the level of the other input.
N * @param CH0P: Specify the CH0 polarity.
N *        This parmeter can be one of the following values:
N *        @arg GPTM_CHP_NONINVERTED : Active high level or rising edge
N *        @arg GPTM_CHP_INVERTED    : Active low level or falling edge
N * @param CH1P: Specify the CH1 polarity.
N *        This parmeter can be one of the following values:
N *        @arg GPTM_CHP_NONINVERTED : Active high level or rising edge
N *        @arg GPTM_CHP_INVERTED    : Active low level or falling edge
N * @retval None
N ************************************************************************************************************/
Nvoid GPTM_DecoderConfig(AM_GPTM_TypeDef* GPTMx, GPTM_SMSEL_Enum DecoderMod,
N                        GPTM_CHP_Enum CH0P, GPTM_CHP_Enum CH1P)
N{
N  u32 wMdcfr, wCh0Icfr, wCh1Icfr, wChpolr;
N
N  /* Get the GPTMx MDCFR register value */
N  wMdcfr = GPTMx->MDCFR;
N
N  /* Get the GPTMx CH0ICFR & CH1ICFR register value */
N  wCh0Icfr = GPTMx->CH0ICFR;
N  wCh1Icfr = GPTMx->CH1ICFR;
N
N  /* Get the GPTMx CHPOLR register value */
N  wChpolr = GPTMx->CHPOLR;
N
N  /* Set the decoder mode */
N  wMdcfr &= MDCFR_SMSEL_MASK;
X  wMdcfr &= ~0x00000700ul;
N  wMdcfr |= DecoderMod;
N
N  /* Select the channel 0 and the channel 1 as input and clear CH0SRC */
N  wCh0Icfr &= CHICFR_CHCCS_MASK & (~CH0ICFR_CH0SRC);
X  wCh0Icfr &= ~0x00030000ul & (~0x80000000ul);
N  wCh1Icfr &= CHICFR_CHCCS_MASK;
X  wCh1Icfr &= ~0x00030000ul;
N  wCh0Icfr |= GPTM_CHCCS_DIRECT;
N  wCh1Icfr |= GPTM_CHCCS_DIRECT;
N
N  /* Set the CH0 and the CH1 polarities */
N  wChpolr &= ~(CHPOLR_CH0P | CHPOLR_CH1P);
X  wChpolr &= ~(0x00000001ul | 0x00000004ul);
N  wChpolr |= (CH0P | (CH1P << 2));
N
N  /* Write to GPTMx MDCFR */
N  GPTMx->MDCFR = wMdcfr;
N
N  /* Write to GPTMx CH0ICFR & CH1ICFR */
N  GPTMx->CH0ICFR = wCh0Icfr;
N  GPTMx->CH1ICFR = wCh1Icfr;
N
N  /* Write to GPTMx CHPOLR */
N  GPTMx->CHPOLR = wChpolr;
N}
