N                           (x == GPTM_TRSEL_TI1S1) || \
N                           (x == GPTM_TRSEL_ETIF) || \
N                           (x == GPTM_TRSEL_TI0BED) || \
N                           (x == GPTM_TRSEL_ITI0) || \
N                           (x == GPTM_TRSEL_ITI1) || \
N                           (x == GPTM_TRSEL_ITI2))
X#define IS_GPTM_TRSEL(x) ((x == GPTM_TRSEL_UEVG) ||                            (x == GPTM_TRSEL_TI0S0) ||                            (x == GPTM_TRSEL_TI1S1) ||                            (x == GPTM_TRSEL_ETIF) ||                            (x == GPTM_TRSEL_TI0BED) ||                            (x == GPTM_TRSEL_ITI0) ||                            (x == GPTM_TRSEL_ITI1) ||                            (x == GPTM_TRSEL_ITI2))
N
N/* check parameter of the ITI */
N#define IS_GPTM_ITI(x)  ((x == GPTM_TRSEL_ITI0) || (x == GPTM_TRSEL_ITI1) || (x == GPTM_TRSEL_ITI2))
N
N/* check parameter of the GPTM_TRSEL for \ref GPTM_ChExternalClockConfig function */
N#define IS_GPTM_TRSEL_CH(x)  ((x == GPTM_TRSEL_TI0S0) || (x == GPTM_TRSEL_TI1S1) || \
