N/*********************************************************************************************************//**
N * @brief Configure the CHx as input.
N * @param GPTMx: where GPTMx is the selected GPTM from the GPTM peripheral, x can be 0~3.
N * @param Ch: Specify the GPTM Channel.
N *        This parmeter can be one of the following values:
N *        @arg GPTM_CH_0 : GPTM channel 0
N *        @arg GPTM_CH_1 : GPTM channel 1
N *        @arg GPTM_CH_2 : GPTM channel 2
N *        @arg GPTM_CH_3 : GPTM channel 3
N * @param Pol: The input polarity.
N *        This parameter can be one of the following values:
N *        @arg GPTM_CHP_NONINVERTED : Active high level or rising edge
N *        @arg GPTM_CHP_INVERTED    : Active low level or falling edge
N * @param Sel: Specify the input to be used.
N *        This parameter can be one of the following values:
N *        @arg GPTM_CHCCS_DIRECT   : GPTM CHxI is mapped on CHx.
N *        @arg GPTM_CHCCS_INDIRECT : GPTM CH1I is mapped on CH0 (or CH0I->CH1 or CH2I->CH3 or CH3I->CH2).
N *        @arg GPTM_CHCCS_TRCED    : GPTM CHx is mapped on TRC.
N * @param Filter: Specify the input capture filter.
N *        This parameter must be a value between 0x00 and 0x0F.
N * @retval None
N ************************************************************************************************************/
Nstatic void
Ngptm_CHx_Config(AM_GPTM_TypeDef* GPTMx, GPTM_CH_Enum Ch, GPTM_CHP_Enum Pol, GPTM_CHCCS_Enum Sel, u8 Filter)
N{
N  vu32* pIcfr = (vu32*)&GPTMx->CH0ICFR + Ch;
N  u32 wIcfr, wChpolr;
N
N  /* Disable the channel N: reset the CHxE bit */
N  GPTMx->CHCTR &= ~((u32)0x1 << (Ch << 1));
N
N  wIcfr = *pIcfr;
N  wChpolr = GPTMx->CHPOLR;
N
N  /* Select the input and set the filter */
N  wIcfr &= CHICFR_CHCCS_MASK & CHICFR_CHF_MASK;
X  wIcfr &= ~0x00030000ul & ~0x0000000Ful;
N  wIcfr |= Sel | Filter;
N  *pIcfr = wIcfr;
N
N  /* Select the polarity bit */
N  wChpolr &= ~((u32)0x1 << (Ch << 1));
N  wChpolr |= (u32)Pol << (Ch << 1);
N  GPTMx->CHPOLR = wChpolr;
N
N  /* Set the CHxE Bit */
N  GPTMx->CHCTR |= (u32)0x1 << (Ch << 1);
N}
