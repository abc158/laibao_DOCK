N *        @arg GPTM_ETIPSC_2   : ETIP frequency divided by 2
N *        @arg GPTM_ETIPSC_4   : ETIP frequency divided by 4
N *        @arg GPTM_ETIPSC_8   : ETIP frequency divided by 8
N * @param Pol: The external trigger input polarity.
N *        It can be one of the following values:
N *        @arg GPTM_ETIPOL_NONINVERTED : Active high level or rising edge
N *        @arg GPTM_ETIPOL_INVERTED    : Active low level or falling edge
N * @param Filter: Filter for ETI input.
N *        This parameter must be a value between 0x00 and 0x0F
N * @retval None
N ************************************************************************************************************/
Nvoid GPTM_EtiConfig(AM_GPTM_TypeDef* GPTMx, GPTM_ETIPSC_Enum Psc, GPTM_ETIPOL_Enum Pol, u8 Filter)
N{
N  u32 wTrcfr;
N
N  /* Get TRCFR value with cleared ETI configuration bits */
N  wTrcfr = GPTMx->TRCFR & TRCFR_ETI_CONF_MASK;
X  wTrcfr = GPTMx->TRCFR & ~0x00013F00ul;
N
N  /* Set the prescaler, filter and polarity for ETI inpit */
N  wTrcfr |= (u32)Psc | Pol | ((u32)Filter << 8);
N
N  /* Write to GPTMx TRCFR */
N  GPTMx->TRCFR = wTrcfr;
N}
