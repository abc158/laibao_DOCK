N/*********************************************************************************************************//**
N * @brief Select the STI source.
N * @param GPTMx: where GPTMx is the selected GPTM from the GPTM peripheral, x can be 0~3.
N * @param Sel: Store the STI source.
N *        This parameter can be one of the following:
N *        @arg GPTM_TRSEL_ITI0  : Internal trigger 0.
N *        @arg GPTM_TRSEL_ITI1  : Internal trigger 1.
N *        @arg GPTM_TRSEL_ITI2  : Internal trigger 2.
N *        @arg GPTM_TRSEL_TI0BED : TI0 both edge detector.
N *        @arg GPTM_TRSEL_TI0S0 : Filtered channel 0 input.
N *        @arg GPTM_TRSEL_TI1S1 : Filtered channel 1 input.
N *        @arg GPTM_TRSEL_ETIF  : External trigger input.
N *        @arg GPTM_TRSEL_UEVG  : Trigger by setting UEVG bit.
N * @retval None
N ************************************************************************************************************/
Nvoid GPTM_StiConfig(AM_GPTM_TypeDef* GPTMx, GPTM_TRSEL_Enum Sel)
N{
N  u32 wTrcfr;
N
N  /* Disable slave mode */
N  GPTMx->MDCFR &= MDCFR_SMSEL_MASK;
X  GPTMx->MDCFR &= ~0x00000700ul;
N
N  /* Get the TRCFR value with cleared TRSEL */
N  wTrcfr = GPTMx->TRCFR & TRCFR_TRSEL_MASK;
X  wTrcfr = GPTMx->TRCFR & ~0x0000000Ful;
N
N  /* Set the STI source */
N  GPTMx->TRCFR |= wTrcfr | Sel;
